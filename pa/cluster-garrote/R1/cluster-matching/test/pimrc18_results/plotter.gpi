set title "Mapping costs"
set ylab "cost"
set xlab "#clusters"

set xrange [0.5:7.5]
set yrange [90:310]
set term eps
set boxwidth 0.9
set output "cluster-garrote-costs.eps"
set grid 
set xtics nomirror
set ytics nomirror
set key right top
#set border 3 # remove right and up border

# http://colorbrewer2.org/?type=qualitative&scheme=Dark2&n=5
set style line 1 lw 2 pt 7 pi -1 ps 0.5 lc "#7570b3" # blue
set style line 2 lw 2 pt 7 pi -1 ps 1 lc "#1b9e77" # green
set style line 3 lw 2 pt 7 pi -1 ps 1 lc "#d95f02" # orange
set style line 4 lw 2 pt 7 pi -1 ps 1 lc "#e7298a" # pink
set style line 5 lw 2 pt 7 pi -1 ps 1 lc "#66a61e" # green prairie
set pointintervalbox 1.5


plot "num_clusters_and_costs.txt" using 1:2 every ::1::8 with linespoints ls 1 lw 5 title "clustering+garrote",\
    "num_clusters_and_costs.txt" using 1:3 every ::1::8 with lines title "GA cost" ls 2 lw 7 dt 1,\
    "num_clusters_and_costs.txt" using 1:4 every ::1::8 with lines title "GA lat" ls 3 lw 7 dt 2,
    

